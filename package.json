    "webgl-texture3d": "0.x.x"
  },
  "devDependencies": {
    "painterly-textures": "0.0.3",
    "voxel": "~0.2.0",
    "toolbar": "0.0.5",
    "voxel-player": "~0.1.0",
    "voxel-pp": "0.0.4",
    "voxel-engine": "~0.8.1",
    "browserify": "~2.4.0",
    "serve": "~0.1.2"
  }
